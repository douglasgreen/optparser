{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": "off",
        "indent": ["error", 4],
        "quotes": "off",
        "semi": "off",
        "space-before-function-paren": "off"
    }
}
